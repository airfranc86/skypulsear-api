{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": "public",
  "installCommand": null,
  "framework": null,
  "rewrites": [
    {
      "source": "/",
      "destination": "/dashboard.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
